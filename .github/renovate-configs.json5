{
  "extends": ["config:best-practices"],
  "onboarding": false,
  "platform": "github",
  "forkProcessing": "enabled",
  "dockerfile": {
    "fileMatch": ["(^|/|\.|.*)([Dd]ocker|[Cc]ontainer)file$"]
  }
  "packageRules": [
    {
      "enabled": true,
      "matchDatasources": [
        "docker"
      ]
    }
  ],
}
